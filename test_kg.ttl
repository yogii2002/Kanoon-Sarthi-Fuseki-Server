 nyon:hasStartIndex 1954; 
 nyon:hasEndIndex 2184

.

fact:199909KS1SCF4 rdfs:type nyon:Fact;
 	rdfs:label "The pleadings raised in this regard"@en ;
 nyon:hasStartIndex 2486; 
