	 nyon:hasProvision section:7_1_SC_LICENSE_AGREEMENT ;
	 nyon:hasProvision section:CLAUSEE_14_LICENSE_AGREEMENT ;
	
.

fact:200604KS1SCF4 rdfs:type nyon:Fact;
